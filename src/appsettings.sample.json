{
  "Authorization": {
    "Issuer": "auth.host.com",
    "Audience": "YOUR_AUDIENCE",
    "SecretKey": "YOUR_SECRET_KEY"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=SQL_SERVER_INSTANCE;Database=DATABASE_NAME;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "Sentry": {
    "Dsn": "https://<key>:<secret>@sentry.io/<project>"
  },
  "Smtp": {
    "Name": "YOUR_NAME",
    "Email": "YOUR_EMAIL",
    "Username": "YOUR_USERNAME",
    "Password": "YOUR_PASSWORD",
    "Host": "smtp.host-example.com",
    "Port": 465,
    "UseSsl": false
  }
}